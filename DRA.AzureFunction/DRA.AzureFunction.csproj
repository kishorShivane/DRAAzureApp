  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DRA.BusinessLogic\DRA.BusinessLogic.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Helpers\" />
  </ItemGroup>
